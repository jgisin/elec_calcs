 <div class="row">
    <div class="col-sm-7">
      <div class = "page-header"><h2>Fault Current Values</h2></div>
<%= link_to "View Panels", {controller: 'panels', action: 'index', project_id: @project.id}, class: "btn btn-md btn-default", remote: true   %> </p>
      <table class="table table-hover" id = "panel-table">
        <thead>
          <tr>
            <th>Panel Name</th>
            <th>Wire Length</th>
            <th>Wire Size</th>
            <th>Voltage Loss</th>
          </tr>
        </thead>

        <tbody>
          <% @panels.each do |panel| %>
          <tr>
            <td><%= panel.panel_name %></td>
            <td><%= panel.wire_length %></td>
            <td><%= "##{panel.wire_size} #{panel.wire_type == 0 ? "CU" : "AL"}" %></td>
            <td><%= "#{number_with_precision(panel.voltdrop, precision: 2)}%" %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>