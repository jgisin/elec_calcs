 <div class="row">
    <div class="col-sm-7">
      <div class = "page-header"><h2>Fault Current Values</h2></div>
<%= link_to "View Units", {controller: 'units', action: 'index', project_id: @project.id}, class: "btn btn-md btn-default", remote: true   %> </p>
      <table class="table table-hover" id = "panel-table">
        <thead>
          <tr>
            <th>Unit Name</th>
            <th>General Lighting Load</th>
            <th>Demand Factor</th>
            <th>Net Computed Load</th>
          </tr>
        </thead>

        <tbody>
          <% @units.each do |unit| %>
          <tr>
            <td><%= unit.unit_name %></td>
            <td><%= "#{unit.sqr_ftg} @ 3 Volt-Amperes per Sq. Ft. + Small Appliance Load + Washer Load:
            Total Computed Load = #{unit.total_unit}" %></td>
            <td><%=  %></td>
            <td><%= %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>